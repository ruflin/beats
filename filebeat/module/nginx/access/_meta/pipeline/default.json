{
  "description": "pipeline for parsing Nginx logs",
  "processors": [{
    "grok": {
      "field": "message",
      "patterns":[
        "%{IPORHOST:nginx.access.remote_ip} - %{DATA:nginx.access.user_name} \\[%{HTTPDATE:nginx.access.time}\\] \"%{WORD:nginx.access.request_action} %{DATA:nginx.access.request} HTTP/%{NUMBER:nginx.access.http_version}\" %{NUMBER:nginx.access.response} %{NUMBER:nginx.access.bytes} \"%{DATA:nginx.access.referrer}\" \"%{DATA:nginx.access.agent}\""
        ],
      "ignore_missing": true
    }
  },{
    "remove":{
      "field": "message"
    }
  }, {
    "user_agent": {
      "field": "agent"
    }
  }, {
    "remove": {
      "field": "agent"
    }
  }, {
    "geoip": {
      "field": "remote_ip"
    }
  }]
}
