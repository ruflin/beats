{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": false
      },
      "_meta": {
        "version": "0.0.1"
      },
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "nginx": {
          "properties": {
            "access": {
              "properties": {
                "bytes": {
                  "type": "long"
                },
                "http_version": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "referrer": {
                  "norms": false,
                  "type": "text"
                },
                "remote_ip": {
                  "type": "ip"
                },
                "request": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "request_action": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "response": {
                  "type": "long"
                },
                "user_name": {
                  "norms": false,
                  "type": "text"
                }
              }
            }
          }
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.refresh_interval": "5s"
  },
  "template": "testoutput-*"
}